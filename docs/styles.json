    "css": {
      "*": {
        "box-sizing": "border-box",
        "margin": "0",
        "padding": "0"
      },
      "body": {
        "font-family": "'Roboto', sans-serif",
        "color": "#333333"
      },
      "header": {
        "background-color": "#173459",
        "color": "rgb(221, 221, 221)",
        "padding": "0.5em 1em"
      },
      "nav": {
        "position": "fixed",
        "top": "0",
        "left": "0",
        "width": "100%",
        "background-color": "#173459",
        "display": "flex",
        "justify-content": "space-between",
        "align-items": "center",
        "padding": "1px 20px",
        "z-index": "1000"
      },
      "nav .logo": {
        "height": "40px",
        "margin-right": "10px"
      },
      "nav .company-name": {
        "color": "rgb(221, 221, 221)",
        "font-size": "1rem"
      },
      "nav ul": {
        "list-style": "none",
        "display": "flex",
        "gap": "0.5em",
        "margin": "0",
        "padding": "0"
      },
      "nav ul li": {
        "margin": "0"
      },
      "nav ul li a": {
        "color": "rgb(221, 221, 221)",
        "text-decoration": "none",
        "transition": "color 0.3s ease"
      },
      "nav ul li a:hover": {
        "color": "#ff5a1f"
      },
      "nav ul li a:visited, nav ul li a:focus, nav ul li a:active": {
        "color": "rgb(221, 221, 221) !important",
        "text-decoration": "none !important"
      },
      ".hero": {
        "position": "relative",
        "width": "100%",
        "height": "200px",
        "overflow": "hidden",
        "background-color": "#173459",
        "text-align": "center",
        "color": "rgb(221, 221, 221)"
      },
      ".hero-image": {
        "position": "absolute",
        "top": "0",
        "left": "0",
        "width": "100%",
        "height": "100%",
        "z-index": "1",
        "opacity": "100%"
      },
      ".hero-gif": {
        "width": "100%",
        "height": "100%",
        "object-fit": "cover",
        "opacity": "80%"
      },
      ".hero-text": {
        "position": "absolute",
        "top": "50%",
        "left": "50%",
        "transform": "translate(-50%, -50%)",
        "z-index": "2",
        "padding": "1em",
        "border-radius": "10px",
        "font-family": "'Cinzel', serif",
        "font-size": "calc(12px + 2vw)",
        "text-shadow": "-1px -1px 0 #000, 1px -1px 0 #000, -1px 1px 0 #000, 1px 1px 0 #000"
      },
      ".hero-text h1": {
        "margin": "0"
      },
      ".hero-logo": {
        "margin-top": "1em",
        "width": "70px",
        "height": "70px",
        "background-image": "url('logo.svg')",
        "background-size": "cover",
        "position": "relative",
        "transform": "translate(-50%, -50%)",
        "animation": "spin 10s linear infinite",
        "justify-content": "center",
        "align-items": "center"
      },
      "@keyframes spin": {
        "from": { "transform": "rotate(0deg)" },
        "to": { "transform": "rotate(360deg)" }
      },
      ".section": {
        "padding": "2em 2em",
        "border-top": "1px solid #ddd",
        "text-align": "center",
        "padding-bottom": "20px"
      },
      ".section:nth-child(even)": {
        "background-color": "#f0f0f0"
      },
      ".content": {
        "max-width": "1000px",
        "margin": "0 auto",
        "padding": "0 1em"
      },
      "h2": {
        "font-size": "2em",
        "margin-bottom": "0.5em",
        "color": "#173459"
      },
      ".services": {
        "position": "relative",
        "padding-top": "1em",
        "padding-bottom": "1em"
      },
      ".services-image": {
        "position": "absolute",
        "top": "0",
        "left": "0",
        "width": "100%",
        "height": "100%",
        "background-image": "url('')",
        "background-size": "cover",
        "opacity": "100%",
        "z-index": "-1",
        "padding-top": "3em",
        "background-color": "#173459"
      },
      ".card": {
        "background-color": "white",
        "border-radius": "8px",
        "box-shadow": "1 1 10px rgba(0, 0, 0, 0.1)",
        "padding": "1em",
        "text-align": "center",
        "padding-top": "1em",
        "padding-bottom": "1em",
        "box-shadow": "0 4px 8px rgba(0, 0, 0, 0.2)"
      },
      ".card-list": {
        "background-color": "white",
        "border-radius": "8px",
        "box-shadow": "0 0 10px rgba(0, 0, 0, 0.1)",
        "padding": "1em",
        "text-align": "center",
        "padding-top": "1em",
        "padding-bottom": "1em",
        "justify-content": "center",
        "align-items": "center",
        "align-content": "center",
        "transition": "transform 0.3s ease-in-out"
      },
      ".service-item:hover .card-list": {
        "transform": "scale(1.2)"
      },
      ".card-avail": {
        "background-color": "white",
        "border-radius": "8px",
        "box-shadow": "0 0 10px rgba(0, 0, 0, 0.1)",
        "padding": ".5em",
        "text-align": "center",
        "width": "fit-content",
        "height": "auto",
        "margin": "auto",
        "margin-bottom": "1em",
        "position": "relative"
      },
      ".card p": {
        "text-align": "justify",
        "margin-bottom": "0.5em"
      },
      ".services-list": {
        "display": "grid",
        "grid-template-columns": "repeat(auto-fit, minmax(200px, 1fr))",
        "gap": "1em",
        "justify-items": "center",
        "align-content": "center",
        "padding-right": "1em",
        "padding-left": "1em"
      },
      ".service-item": {
        "width": "100%",
        "justify-content": "center",
        "align-content": "center",
        "align-items": "center",
        "flex": "1 1 200px",
        "transition": "transform 0.3s ease-in-out"
      },
      ".loc-image": {
        "position": "absolute",
        "top": "0",
        "left": "0",
        "width": "100%",
        "height": "100%"
      },
      ".local-image": {
        "position": "relative",
        "top": "0",
        "left": "0",
        "width": "100%",
        "height": "100%",
        "background-image": "url('')",
        "background-size": "cover",
        "opacity": "100%",
        "z-index": "-1",
        "padding-top": "1em",
        "padding-bottom": "1em",
        "opacity": "100%",
        "background-color": "#173459"
      },
      ".where": {
        "display": "flex",
        "align-content": "left"
      },
      ".who": {
        "display": "flex"
      },
      ".about-content, .location-content, .philosophy-content": {
        "display": "flex",
        "flex-wrap": "wrap",
        "justify-content": "center",
        "gap": "1em",
        "text-align": "center"
      },
      ".about-text, .location-text ul, .philosophy-text": {
        "flex": "1 1 40%",
        "max-width": "500px",
        "list-style": "none",
        "text-decoration": "none"
      },
      ".about-text ul": {
        "text-decoration": "none",
        "list-style": "none"
      },
      ".about-image-wrapper, .location-image-wrapper": {
        "width": "100%",
        "max-width": "250px",
        "margin": "0 auto"
      },
      ".about-image, .location-image": {
        "width": "100%",
        "height": "auto",
        "display": "block",
        "border-radius": "8px",
        "box-shadow": "0 4px 8px rgba(0, 0, 0, 0.5)"
      },
      ".careerwrap": {
        "display": "flex",
        "flex-wrap": "wrap",
        "justify-content": "center",
        "align-items": "center",
        "gap": "1em"
      },
      ".form": {
        "background-color": "rgb(218, 215, 215)",
        "padding-top": "20px"
      },
      ".formz": {
        "background-color": "#173459",
        "padding-top": "20px",
        "padding-bottom": "20px"
      },
      ".servicez": {
        "position": "absolute",
        "top": "0",
        "left": "0",
        "width": "100%",
        "height": "100%",
        "background-image": "url('')",
        "background-size": "cover",
        "opacity": "100%",
        "z-index": "-1",
        "padding-top": "3em",
        "background-color": "white"
      },
      "section#contact .content iframe": {
        "position": "absolute",
        "z-index": "1",
        "pointer-events": "auto"
      },
      ".foot": {
        "height": "10px"
      },
      ".career-text": {
        "position": "absolute",
        "top": "50%",
        "left": "50%",
        "transform": "translate(-50%, -50%)",
        "z-index": "2",
        "padding": "1em",
        "border-radius": "10px",
        "font-family": "'Cinzel', serif",
        "font-size": "calc(10px + 1vw)",
        "text-shadow": "-1px -1px 0 #000, 1px -1px 0 #000, -1px 1px 0 #000, 1px 1px 0 #000",
        "list-style": "none",
        "text-decoration": "none"
      },
      ".form-logo": {
        "position": "relative",
        "width": "90%",
        "height": "90%",
        "z-index": "1",
        "background-size": "cover",
        "transform": "translate(-50%, -50%)",
        "animation": "spin 60s linear infinite",
        "justify-self": "center",
        "align-self": "center",
        "justify-items": "center"
      },
      ".apply-button": {
        "background-color": "#007bff",
        "color": "white",
        "font-size": "18px",
        "padding": "10px 20px",
        "border": "none",
        "border-radius": "5px",
        "cursor": "pointer",
        "transition": "background-color 0.3s ease",
        "box-shadow": "1 1 10px rgba(0, 0, 0, 0.1)"
      },
      ".apply-button:hover": {
        "background-color": "#0056b3"
      },
      "@keyframes slideInFromLeft": {
        "0%": {
          "transform": "translateX(-100%)",
          "opacity": "0"
        },
        "100%": {
          "transform": "translateX(0)",
          "opacity": "1"
        }
      },
      ".slide-in": {
        "animation": "slideInFromLeft 1s ease-out forwards"
      },
      ".slide-element": {
        "opacity": "0"
      },
      "@keyframes slideInFromRight": {
        "0%": {
          "transform": "translateX(+100%)",
          "opacity": "0"
        },
        "100%": {
          "transform": "translateX(0)",
          "opacity": "1"
        }
      },
      ".slide-inR": {
        "animation": "slideInFromRight 1s ease-out forwards"
      },
      ".slide-elementR": {
        "animation": "slideInFromRight 1s ease-out forwards",
        "opacity": "0"
      }
    },
    "animate": {
      "slideInFromLeft": {
        "0%": {
          "transform": "translateX(-100%)",
          "opacity": "0"
        },
        "100%": {
          "transform": "translateX(0)",
          "opacity": "1"
        }
      },
      "slideInFromRight": {
        "0%": {
          "transform": "translateX(+100%)",
          "opacity": "0"
        },
        "100%": {
          "transform": "translateX(0)",
          "opacity": "1"
        }
      },
      "spin": {
        "from": {
          "transform": "rotate(0deg)"
        },
        "to": {
          "transform": "rotate(360deg)"
        }
      }
    }
  }
